
 # NOTE: Cloud credentials should be set in environment variables.
#       https://www.terraform.io/docs/providers/aws/index.html#environment-variables
# These variables should be set, at minimum. See `variables.tf` for others.
gcp_project = description = "GCP project name"
gcp_region = description = "GCP region, e.g. europe-west3"
crypto_key = description = "Crypto key name to create under the key ring"
keyring_location =  description = "Keyring location  default = "global"
instance_type_server = description = "GCP machine type"
instance_type_worker = description = "GCP machine type"
cust_name = description = "Customer name"
owner = "exampleowner"
ssh_user = "Your-GCP-User-Name-for-SSH"
primary_namespace = "primary-demostack"
public_key = "ssh-rsa AAAAB3NzaC1y"
demo_username = "demo"
demo_password = "demo"
servers = 3
workers = 3
vaultlicense = "aaaaaaaaaa"
consullicense = "bbbbbbbbb"
nomadlicense = "cccccccccc"
enterprise = false
#---------------------------------------------------
# To specify a version different from the default
#---------------------------------------------------
 vault_url = "https://releases.hashicorp.com/vault/0.11.1/vault_0.11.1_linux_amd64.zip"